{
    "name": "Bomnibox - Better Omnibox",
    "short_name": "bomnibox",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "__MSG_extDesc__",
    "homepage_url": "https://github.com/brainfoolong/bomnibox",
    "minimum_chrome_version" : "20.0",
    "author": "BrainFooLong",
    "default_locale" : "en",
    "icons": {
        "16": "img/icon-16.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "omnibox": { "keyword" : "$" },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "history",
        "tabs"
    ],
    "commands": {
        "open": {
            "suggested_key": {
                "default": "Ctrl+Shift+0",
                "mac": "Command+Shift+0"
            },
            "global": true,
            "description": "Open Bomnibox"
        }
    }
}